{
  "host": "localhost",
  "port": 3000,
  "public": "../dist/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "knexDriver": "mysql",
  "nedb": "../data",
  "mysqlClient": {},
  "mysql": {
    "client": "mysql2",
    "connection": "mysql://web:web@localhost:3306/web"
  },
  "postgres": {
    "client": "pg",
    "connection": "postgres://web:web@localhost:5432/web"
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "localhost",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    },
    "github": {
      "clientID": "your github client id",
      "clientSecret": "your github client secret",
      "successRedirect": "/"
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    }
  }
}
