            .configure(feathers.authentication({
                header: 'Authorization',
                path: '/authentication',
                jwtStrategy: 'jwt',
                entity: 'user',
                service: 'users',
                storageKey: 'feathers-jwt',
                storage: window.localStorage // Passing a WebStorage-compatible object to enable automatic storage on the client.
            }));
    </script>

