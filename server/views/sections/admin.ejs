
        <!-- Admin -->
        <div class="container is-fluid">
            <h1 class="title is-3">Admin</h1>
            <div class="columns is-page">
                <!-- Players -->
                <div class="column is-4">
                    <h2 class="subtitle">Player</h2>
                    <div v-for="(player, index) in allPlayers" class="media">
                        <div class="media-left">
                            <figure class="image is-48x48">
                                <img src="https://bulma.io/images/placeholders/48x48.png" alt="img">
                            </figure>
                        </div>
                        <div class="media-content">
                            <p class="is-bold">{{ player.display_name }}</p>
                            <p>{{ player.description }}</p>
                        </div>
                        <div class="buttons has-addons is-pulled-right">
                            <div class="button is-small">Edit</div>
                            <div class="button is-small">Friend</div>
                        </div>
                    </div>
                    <hr>
                    <player-form></player-form>
                </div>

                <!-- Locations -->
                <div class="column is-4">
                    <h2 class="subtitle">Locations</h2>
                    <div v-for="(item, index) in allLocations" class="media">
                        <div class="media-left">
                            <figure class="image is-48x48">
                                <img src="https://bulma.io/images/placeholders/48x48.png" alt="img">
                            </figure>
                        </div>
                        <div class="media-content">
                            <p class="is-bold">{{ item.display_name }}</p>
                            <p>{{ item.description }}</p>
                        </div>
                        <div class="buttons has-addons is-pulled-right">
                            <div class="button is-small">Edit</div>
                            <div class="button is-small">Discover</div>
                            <div class="button is-small">Explore</div>
                            <div class="button is-small">Explore All</div>
                        </div>
                    </div>
                    <hr>
                    <location-form :locations="allLocations"></location-form>
                </div>

                <!-- Cards -->
                <div class="column is-4">
                    <h2 class="subtitle">Cards</h2>
                    <div v-for="(item, index) in allCards" class="media">
                        <div class="media-left">
                            <figure class="image is-48x48">
                                <img src="https://bulma.io/images/placeholders/48x48.png" alt="img">
                            </figure>
                        </div>
                        <div class="media-content">
                            <p class="is-bold">{{ item.display_name }}</p>
                            <p>{{ item.description }}</p>
                        </div>
                        <div class="buttons has-addons is-pulled-right">
                            <div class="button is-small">Edit</div>
                            <div class="button is-small">Discover</div>
                            <div class="button is-small">Unlock</div>
                         </div>
                    </div>
                    <hr>
                    <card-form :locations="allLocations"></card-form>
                </div>
            </div>
        </div>